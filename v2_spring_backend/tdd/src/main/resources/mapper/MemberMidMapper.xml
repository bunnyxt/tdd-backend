<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bunnyxt.tdd.dao.MemberMidDao">

    <select id="queryMemberMidMaxId" resultType="Integer">
        select
        id
        from
        tdd_member_mid
        order by
        id desc
        limit 1
    </select>

    <select id="queryMemberMidById" resultType="Integer" parameterType="Integer">
        select
        mid
        from
        tdd_member_mid
        where
        id = #{id}
    </select>

    <select id="queryMemberMidsByIds" resultType="Integer">
        select
        mid
        from
        tdd_member_mid
        where id in
        <foreach collection="list" item="id" index="index" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </select>
</mapper>